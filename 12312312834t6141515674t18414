                            wait(.2)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(_G.GivePotion)
                        end
                        end)


                        LoopGive.MouseButton1Click:Connect(function()
                            if LoopGive.Text == 'LoopGive' then
                                LoopGive.Text = 'UnLoopGive'
                        
                            local giver = Player(Target.Text)
                            local player = game.Players.LocalPlayer
                        
                        
                                local Loop
                                local loopFunction = function()
                        
                                    
                                if not game.Players.LocalPlayer.Backpack:FindFirstChild("potion") then
                                end     
                                
                                if game.Players.LocalPlayer.Backpack:FindFirstChild("potion") then
                                    local v = game.Players.LocalPlayer.Backpack:FindFirstChild("Push")
                                    if v then v:Destroy()
                                        wait()
                                    end
                                    local cmdp = game:GetService("Players")
                                    local cmdlp = cmdp.LocalPlayer
                                    
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = giver.Character:FindFirstChild("HumanoidRootPart").CFrame
                                    wait(0.1)
                                    player.Character.Humanoid:UnequipTools()
                                    local Humanoid = player.Character.Humanoid:Clone()
                                    local Tool = player.Backpack:FindFirstChildOfClass("Tool")
                                    player.Character.Animate.Disabled = false
                                    player.Character.Humanoid:Destroy()
                                    Humanoid.Parent = player.Character
                                    Tool.Parent = player.Character
                                    game.Players.LocalPlayer.Character.Humanoid:ChangeState(10)
                                    firetouchinterest(giver.Character.HumanoidRootPart, Tool.Handle, 0);
                                    wait(0.1)
                                    cmdlp.Character:Remove()
                                    game.Players.localPlayer.CharacterAdded:Wait()
                                end
                                end
                                local Start = function()    
                                    OldFlingPos = player.Character.HumanoidRootPart.Position
                                    
                                    Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
                                end;
                                local Pause = function()
                                    Loop:Disconnect()     
                            end;
                                Start()	
                                repeat wait() until LoopGive.Text == 'LoopGive'
                                Pause()
                            else
                                LoopGive.Text = 'LoopGive'
                            end
                        end)



