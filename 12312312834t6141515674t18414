                    local Value = tostring(PingStats)
                    local PingValue = Value:split(" ")
                    local PingNumber = tonumber(PingValue[1])
            
                    if PingNumber < 130 then
                        PuppywareSettings.Aiming.AimingSettings.GetVelocity = PingNumber / 1000 + 0.037
