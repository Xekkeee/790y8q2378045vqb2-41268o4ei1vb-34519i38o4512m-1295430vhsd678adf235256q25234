Tab:AddButton({Name = "QuickFling",Callback = function()
    if Select then
    local Loop
    local OldFlingPos = player.Character.HumanoidRootPart.Position
    local loopFunction = function()
        local success,err = pcall(function()
            local FlingEnemy = game.Players[Select].Character
            if FlingEnemy and player.Character then
                FlingTorso = FlingEnemy.UpperTorso
                local dis = 3.85
                local increase = 6
                if FlingEnemy.Humanoid.MoveDirection.X < 0 then
                    xchange = -increase
                elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
                    xchange = increase
                else
                    xchange = 0
                end
                if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                    zchange = -increase
                elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                    zchange = increase
                else
                    zchange = 0
                end          
                if player.Character then
                    player.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
                    player.Character.HumanoidRootPart.CFrame = CFrame.new(FlingTorso.Position.X + math.random(-dis,dis) + xchange, FlingTorso.Position.Y, FlingTorso.Position.Z + math.random(-dis,dis) + zchange) * CFrame.Angles(math.rad(player.Character.HumanoidRootPart.Orientation.X + 350),math.rad(player.Character.HumanoidRootPart.Orientation.Y + 200),math.rad(player.Character.HumanoidRootPart.Orientation.Z + 240))
                    player.Character.HumanoidRootPart.Velocity = Vector3.new(50000,50000,50000)
                end
            end
        end)
        if err then
            print('fling error : ' .. err)
        end
    end;
    local Start = function()    
        OldFlingPos = player.Character.HumanoidRootPart.Position
        Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
    end;
    local Pause = function()
        Loop:Disconnect()
        local vectorZero = Vector3.new(0, 0, 0)
        player.Character.PrimaryPart.Velocity = vectorZero
        player.Character.PrimaryPart.RotVelocity = vectorZero
        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        local vectorZero = Vector3.new(0, 0, 0)
        player.Character.PrimaryPart.Velocity = vectorZero
        player.Character.PrimaryPart.RotVelocity = vectorZero
        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        local vectorZero = Vector3.new(0, 0, 0)
        player.Character.PrimaryPart.Velocity = vectorZero
        player.Character.PrimaryPart.RotVelocity = vectorZero
        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        local vectorZero = Vector3.new(0, 0, 0)
        player.Character.PrimaryPart.Velocity = vectorZero
        player.Character.PrimaryPart.RotVelocity = vectorZero
        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        local vectorZero = Vector3.new(0, 0, 0)
        player.Character.PrimaryPart.Velocity = vectorZero
        player.Character.PrimaryPart.RotVelocity = vectorZero
        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        local vectorZero = Vector3.new(0, 0, 0)
        player.Character.PrimaryPart.Velocity = vectorZero
        player.Character.PrimaryPart.RotVelocity = vectorZero
        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        local vectorZero = Vector3.new(0, 0, 0)
        player.Character.PrimaryPart.Velocity = vectorZero
        player.Character.PrimaryPart.RotVelocity = vectorZero
        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        local vectorZero = Vector3.new(0, 0, 0)
        player.Character.PrimaryPart.Velocity = vectorZero
        player.Character.PrimaryPart.RotVelocity = vectorZero
        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))

end;
    Start()
    wait(1)
    Pause()
    game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(12)
end
end    
})

Tab:AddToggle({Name = "FistLock",Default = false,Callback = function(v)
    if v == true then
        if Select then
        Kill = true
        local localPlayer     = game:GetService("Players").LocalPlayer;
        local localCharacter  = localPlayer.Character;

        if Select and game.Players[Select].Character.BodyEffects['K.O'].Value == false then
            pcall(function()
                localCharacter.LeftHand.LeftWrist:Destroy();
                localCharacter.RightHand.RightWrist:Destroy();
            end);
            repeat
                
                localCharacter.LeftHand.CFrame = game.Players[Select].Character.LowerTorso.CFrame;
                localCharacter.RightHand.CFrame = game.Players[Select].Character.LowerTorso.CFrame;
                player.Character.LeftHand.CFrame  = CFrame.new(game.Players[Select].Character.UpperTorso.Position);
                player.Character.RightHand.CFrame = CFrame.new(game.Players[Select].Character.UpperTorso.Position);
                wait();
            until Kill == false
            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').HipHeight = 2
            local c = "" .. Select .. " got Knocked." 
            game.StarterGui:SetCore("SendNotification", {
                Title = "Notification";
                Text = c;
                Duration = 2;
            })
        elseif game.Players[Select].Character.BodyEffects['K.O'].Value == true then
                print("101")
        end;
    end
    elseif v == false then
    Kill = false
    game:GetService("Players").LocalPlayer.Character.LeftHand.CFrame  = game:GetService("Players").LocalPlayer.Character.LeftLowerArm.CFrame
    game:GetService("Players").LocalPlayer.Character.RightHand.CFrame = game:GetService("Players").LocalPlayer.Character.RightLowerArm.CFrame
    end
end    
})

Tab:AddToggle({Name = "Fling",Default = false,Callback = function(v)
    if v == true then
        if Select then
        local Loop
        local OldFlingPos = player.Character.HumanoidRootPart.Position
        Knife = true
        local loopFunction = function()
            local success,err = pcall(function()
                local FlingEnemy = game.Players[Select].Character
                if FlingEnemy and player.Character then
                    FlingTorso = FlingEnemy.UpperTorso
                    local dis = 3.85
                    local increase = 6
                    if FlingEnemy.Humanoid.MoveDirection.X < 0 then
                        xchange = -increase
                    elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
                        xchange = increase
                    else
                        xchange = 0
                    end
                    if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                        zchange = -increase
                    elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                        zchange = increase
                    else
                        zchange = 0
                    end          
                    if player.Character then
                        player.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(FlingTorso.Position.X + math.random(-dis,dis) + xchange, FlingTorso.Position.Y, FlingTorso.Position.Z + math.random(-dis,dis) + zchange) * CFrame.Angles(math.rad(player.Character.HumanoidRootPart.Orientation.X + 350),math.rad(player.Character.HumanoidRootPart.Orientation.Y + 200),math.rad(player.Character.HumanoidRootPart.Orientation.Z + 240))
                        player.Character.HumanoidRootPart.Velocity = Vector3.new(5000000,5000000,5000000)
                    end
                end
            end)
            if err then
                print('fling error : ' .. err)
            end
        end;
        local Start = function()    
            OldFlingPos = player.Character.HumanoidRootPart.Position
            Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
        end;
        local Pause = function()
            Loop:Disconnect()
            local vectorZero = Vector3.new(0, 0, 0)
            player.Character.PrimaryPart.Velocity = vectorZero
            player.Character.PrimaryPart.RotVelocity = vectorZero
            player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
            local vectorZero = Vector3.new(0, 0, 0)
            player.Character.PrimaryPart.Velocity = vectorZero
            player.Character.PrimaryPart.RotVelocity = vectorZero
            player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
            local vectorZero = Vector3.new(0, 0, 0)
            player.Character.PrimaryPart.Velocity = vectorZero
            player.Character.PrimaryPart.RotVelocity = vectorZero
            player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
            local vectorZero = Vector3.new(0, 0, 0)
            player.Character.PrimaryPart.Velocity = vectorZero
            player.Character.PrimaryPart.RotVelocity = vectorZero
            player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
            end;
        
        Start()	
        repeat wait() until Knife == false
        Pause()
        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(12)
        end
elseif v == false then
    Knife = false
end
end
})

Tab:AddToggle({Name = "Flingv2",Default = false,Callback = function(v)
    if v == true then
        if Select then
        Bike = true

                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.8922119140625, 21.999820709228516, -546.8743286132812)
                    wait(0.5)
                    fireclickdetector(game:GetService("Workspace").Ignored.Shop["[DuoBike] - $75"].ClickDetector)
                    fireclickdetector(game:GetService("Workspace").Ignored.Shop["[DuoBike] - $75"].ClickDetector)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.6552124023438, 26.234392166137695, -553.2313842773438)
                    
                    tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(0.5, Enum.EasingStyle.Linear)
                    tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(-832.359375, 21.55292320251465, -552.5506591796875)}):Play()
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.6552124023438, 26.234392166137695, -553.2313842773438)
                    wait(0.4)
                    tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(0.5, Enum.EasingStyle.Linear)
                    tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(-832.359375, 21.55292320251465, -552.5506591796875)}):Play()
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.6552124023438, 26.234392166137695, -553.2313842773438)
                    wait(0.8)
                    tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(0.5, Enum.EasingStyle.Linear)
                    tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(-832.359375, 21.55292320251465, -552.5506591796875)}):Play()
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.6552124023438, 26.234392166137695, -553.2313842773438)
                    wait(0.8)
                    tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(0.5, Enum.EasingStyle.Linear)
                    tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(-832.359375, 21.55292320251465, -552.5506591796875)}):Play()
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.6552124023438, 26.234392166137695, -553.2313842773438)
                    
                    game:FindService("RunService").Heartbeat:wait()
                    game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
                    local Loop
                    local OldFlingPos = player.Character.HumanoidRootPart.Position
                    local loopFunction = function()
            
                        local success,err = pcall(function()
                            local FlingEnemy = game.Players[Select].Character
                            if FlingEnemy and player.Character then
                                FlingTorso = FlingEnemy.UpperTorso
                                local dis = 3.85
                                local increase = 6
                                if FlingEnemy.Humanoid.MoveDirection.X < 0 then
                                    xchange = -increase
                                elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
                                    xchange = increase
                                else
                                    xchange = 0
                                end
                                if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                                    zchange = -increase
                                elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                                    zchange = increase
                                else
                                    zchange = 0
                                end          
                                if player.Character then
                                    player.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
                                    player.Character.HumanoidRootPart.CFrame = CFrame.new(FlingTorso.Position.X + math.random(-dis,dis) + xchange, FlingTorso.Position.Y, FlingTorso.Position.Z + math.random(-dis,dis) + zchange) * CFrame.Angles(math.rad(player.Character.HumanoidRootPart.Orientation.X + 350),math.rad(player.Character.HumanoidRootPart.Orientation.Y + 200),math.rad(player.Character.HumanoidRootPart.Orientation.Z + 240))
                                    player.Character.HumanoidRootPart.Velocity = Vector3.new(1000000,1000000,1000000)
                                end
                            end
                        end)
                        if err then
                            print('fling error : ' .. err)
                        end
                    end;
                    local Start = function()    
                        OldFlingPos = player.Character.HumanoidRootPart.Position
                        Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
                    end;
                    local Pause = function()
                        Loop:Disconnect()
                        local vectorZero = Vector3.new(0, 0, 0)
                        player.Character.PrimaryPart.Velocity = vectorZero
                        player.Character.PrimaryPart.RotVelocity = vectorZero
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
                        local vectorZero = Vector3.new(0, 0, 0)
                        player.Character.PrimaryPart.Velocity = vectorZero
                        player.Character.PrimaryPart.RotVelocity = vectorZero
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
                end;
                Start()	
                repeat wait() until Bike == false
                Pause()
                game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(12)
            end
        elseif v == false then
            Bike = false
        end
end    
})










