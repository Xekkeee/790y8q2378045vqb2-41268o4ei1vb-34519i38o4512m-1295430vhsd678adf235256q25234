Tab:AddToggle({Name = "Reduce Lag",Default = false,Callback = function(v)
    if v == true then
       
        local obj = game.workspace.Ignored
        function XrayOn(obj) --Enables xray
            for _,v in pairs(obj:GetChildren()) do
                if (v:IsA("BasePart")) and not v.Parent:FindFirstChild("Humanoid") then
                    v.LocalTransparencyModifier = 1
                end
            XrayOn(v)
            end
        end
        function XrayOff(obj) --Disables xray
            for _,v in pairs(obj:GetChildren()) do
                if (v:IsA("BasePart")) and not v.Parent:FindFirstChild("Humanoid") then
                    v.LocalTransparencyModifier = 0
                end XrayOff(v)
            end
        end
        local toggle = false
                if toggle == false then
                    toggle = true
                XrayOn(obj)
                else
                    toggle = true
                XrayOff(obj)
                end
        elseif v == false then
            local obj = game.workspace.Ignored
            function XrayOn(obj) --Enables xray
                for _,v in pairs(obj:GetChildren()) do
                    if (v:IsA("BasePart")) and not v.Parent:FindFirstChild("Humanoid") then
                        v.LocalTransparencyModifier = 1
                    end
                XrayOn(v)
                end
            end
            function XrayOff(obj) --Disables xray
                for _,v in pairs(obj:GetChildren()) do
                    if (v:IsA("BasePart")) and not v.Parent:FindFirstChild("Humanoid") then
                        v.LocalTransparencyModifier = 0
                    end XrayOff(v)
                end
            end
            local toggle = true
                    if toggle == false then
                        toggle = true
                    
                    XrayOn(obj)
                    else
                        toggle = true
                    XrayOff(obj)
                end
        end
end
})

