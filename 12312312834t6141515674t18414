Tab:AddToggle({Name = "Boxing Farm",Default = false,Callback = function(v)
    if v == true then
        Box = true
        
        if game.Players.LocalPlayer.Character.Animate:FindFirstChild("idle") then
            game.Players.LocalPlayer.Character.Animate.idle:Destroy()
        end


        local Loop
        local loopFunction = function()
                wait()
                pcall(function()
                    LocalPlayer.Character.HumanoidRootPart.CFrame = Workspace.MAP.Map.ArenaBOX.PunchingBagInGame["pretty ransom"].CFrame * CFrame.new(0, -1, 3)
                    if LocalPlayer.Backpack:FindFirstChild("Combat") then
                        LocalPlayer.Backpack.Combat.Parent = LocalPlayer.Character
                    end
                end)
                game:GetService("VirtualUser"):ClickButton1(Vector2.new())
        end;
        local Start = function()
            Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
        end;
        local Pause = function()
            Loop:Disconnect()
        end;
        Start()	
        repeat wait() until  Box == false
        Pause()
    elseif v == false then
        Box = false
        if MusclePos ~= nil then
            player.Character.HumanoidRootPart.CFrame = MusclePos
            wait(0.2)
            player.Character.HumanoidRootPart.CFrame = MusclePos
             else
        end
    end
end
})



