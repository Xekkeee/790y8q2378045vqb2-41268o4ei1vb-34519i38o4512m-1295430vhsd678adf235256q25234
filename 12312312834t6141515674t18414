            if v then v:Destroy()
            end
            local v = game.Players.LocalPlayer.PlayerGui.MainScreenGui:FindFirstChild("Crew") 
            if v then v.Visible = false
            end
        else
            OrionLib:MakeNotification({
                Name = "Notification!",
                Content = "Please Join The Roblox Group For It To Work! Link Copied To Your Clipboard",
                Time = 5,
            }) 
            setclipboard("https://www.roblox.com/groups/5/Builderman#!/about")
    
           end


        game:GetService('RunService'):BindToRenderStep("fake", 0 , function()

            if game.Players.LocalPlayer then
                local success, err = pcall(function()
                game.Players.LocalPlayer.Character:WaitForChild("BodyEffects")
                    

            game.ReplicatedStorage.MainEvent:FireServer("JoinCrew", 5)
                    pcall(function()
                        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                            if v.Name == '[SprayCan]' then
                                v.Parent = game.Players.LocalPlayer.Character
                            end
                        end
                    end)

                    pcall(function()
                        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                            if v.Name == '[SprayCan]' then
                                v.Handle.Transparency = 1
                            end
                        end
                    end) 
            game.ReplicatedStorage.MainEvent:FireServer("LeaveCrew")

            if game.Players.LocalPlayer.Character:FindFirstChild("Wallet") then
                game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("Wallet"))
            end
                end)
            end
       end)
    elseif v == false then
        game:GetService('RunService'):UnbindFromRenderStep("fake")
        pcall(function()
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v.Name == '[SprayCan]' then
                    v:Destroy()
                end
            end
        end) 




    end
end    
})

