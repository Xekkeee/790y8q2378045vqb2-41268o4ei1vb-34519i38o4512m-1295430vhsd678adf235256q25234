                local column = tabsection:AddColumn({
                    Title = " SellerStuff"
                })
                
                local section = column:Section({
                    Title = "Sellers Config"
                })

                section:Toggle({Text = "Auto Drop",State = false,Callback = function(v) 
                    if v == true then
                        game:GetService('RunService'):BindToRenderStep("AutoDrop", 0 , function()
                        game:GetService("ReplicatedStorage").MainEvent:FireServer("DropMoney", "10000")
                        end)
                    elseif v == false then
                        game:GetService('RunService'):UnbindFromRenderStep("AutoDrop")
                    end 
                end
                })

                section:Toggle({Text = "Cash Aura",State = false,Callback = function(v) 
                    if v == true then
                        game:GetService('RunService'):BindToRenderStep("CashAura", 0 , function()
                            for i,v in pairs(game:GetService('Workspace')['Ignored']['Drop']:GetChildren()) do
                                if v:IsA('Part') then
                                    if (v.Position - player.Character.HumanoidRootPart.Position).Magnitude <= 12 then
                                        fireclickdetector(v:FindFirstChild('ClickDetector'))
                                    end
                                end
                            end
                        end)


                    elseif v == false then
                        game:GetService('RunService'):UnbindFromRenderStep("CashAura")
                    end 
                end
                })

                section:Toggle({Text = "DeleteCash",State = false,Callback = function(v) 
                    if v == true then
                        game:GetService('RunService'):BindToRenderStep("CashAura", 0 , function()
                            for i, v in pairs(game.Workspace.Ignored.Drop:GetDescendants())  do
                                if v.Name == "MoneyDrop" then
                                    v.Transparency = 1
                                end
                            
                            end
                            
                            for i, v in pairs(game.Workspace.Ignored.Drop.MoneyDrop:GetChildren()) do
                                if v.Name == "Decal" then
                                    v:Destroy()
                                end
                            
                            end
                        end)


                    elseif v == false then
                        game:GetService('RunService'):UnbindFromRenderStep("CashAura")
                    end 
                end
                })
















