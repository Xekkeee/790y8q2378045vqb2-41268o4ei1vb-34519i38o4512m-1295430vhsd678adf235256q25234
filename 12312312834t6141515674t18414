                section:Toggle({Text = "Anti Peper",State = false,Callback = function(v) 
                    if v == true then
                        game:GetService('RunService'):BindToRenderStep("Anti-Peper", 0 , function()
                            game.StarterGui.MainScreenGui.PepperSpray:Destroy()
                            game:GetService("Lighting").PepperSprayBlur:Destroy()
                            for _, v in next, game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):GetPlayingAnimationTracks() do
