                        LoopGive.Text = "LoopGive"
                        LoopGive.TextColor3 = Color3.fromRGB(0, 0, 0)
                        LoopGive.TextScaled = true
                        LoopGive.TextSize = 14.000
                        LoopGive.TextWrapped = true

                        UICorner_18.CornerRadius = UDim.new(0, 11)
                        UICorner_18.Parent = LoopGive
                        Frame.Active = true
                        Frame.Draggable = true



                        _G.spectating = false

                        local plr = game.Players.LocalPlayer
                        local Player = function(Ev)
                            if Ev == "" then
                            return nil
                            elseif Ev == "random" then
                            return game:GetService("Players"):GetPlayers()[math.random(1, #game:GetService("Players"):GetPlayers())]
                            else
                            for _, v in pairs(game:GetService("Players"):GetPlayers()) do
                            if v.Name:lower():sub(1, #Ev) == Ev:lower() or v.DisplayName:lower():sub(1, #Ev) == Ev then
                            return v
                            end
                            end
                            end
                        end


                        Target.Focused:Connect(function()
                            while Target:IsFocused() and wait() do
                                local User = Player(Target.Text)
                                if User ~= nil then
                                    ImageLabel.Image = game:GetService("Players"):GetUserThumbnailAsync(User.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
                                end
                            end
                        end)

                        Target.FocusLost:Connect(function()
                            while wait() do
                                if Target:IsFocused() then break end
                                if Target.Text ~= "random" then break end
                                ImageLabel.Image = game:GetService("Players"):GetUserThumbnailAsync(Player(Target.Text).UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
                            end
                        end)


                        Teleport.MouseButton1Click:Connect(function()
                            local Target = Player(Target.Text)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Target.Character:FindFirstChild("UpperTorso").CFrame

                        end)

                        Kill.MouseButton1Click:Connect(function()
                            local Target = Player(Target.Text)
        
                            if not game.Players.LocalPlayer.Backpack:FindFirstChild("potion") then
                            game.StarterGui:SetCore("SendNotification", {
                            Title = "Notification";
                            Text = "No Potion Detected";
                            Duration = 1;
                            })
                            end
                            
                            _G.kill = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                            if game.Players.LocalPlayer.Backpack:FindFirstChild("potion") then
                                game.Players.LocalPlayer.Backpack:FindFirstChild("Push"):Destroy()

                            plr.Character.Humanoid:UnequipTools()
                            local Humanoid = plr.Character.Humanoid:Clone()
                            
                            local Tool = plr.Backpack:FindFirstChildOfClass("Tool")
                            local cmdp = game:GetService("Players")
                            local cmdlp = cmdp.LocalPlayer

                            plr.Character.Animate.Disabled = true
                            plr.Character.Humanoid:Destroy()
                            Humanoid.Parent = plr.Character
                            Tool.Parent = plr.Character
                            game.Players.LocalPlayer.Character.Humanoid:ChangeState(15)

                            firetouchinterest(Target.Character.HumanoidRootPart, Tool.Handle, 0);
                            wait(0.1)
                            cmdlp.Character:Remove()
                            game.Players.localPlayer.CharacterAdded:Wait()
                            wait(.1)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(_G.kill)
                            end
                        end)


                        Bring.MouseButton1Click:Connect(function()
                            local Target = Player(Target.Text)
                            
                                if not game.Players.LocalPlayer.Backpack:FindFirstChild("potion") then
                                game.StarterGui:SetCore("SendNotification", {
                                Title = "Notification";
                                Text = "No Potion Detected";
                                Duration = 1;
                                })
                                end
                        
                        _G.Bring = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                        if game.Players.LocalPlayer.Backpack:FindFirstChild("potion") then
                        
                            game.Players.LocalPlayer.Backpack:FindFirstChild("Push"):Destroy()
                            plr.Character.Humanoid:UnequipTools()
                            local Humanoid = plr.Character.Humanoid:Clone()
                            local Tool = plr.Backpack:FindFirstChildOfClass("Tool")
                            local cmdp = game:GetService("Players")
                            local cmdlp = cmdp.LocalPlayer
                            plr.Character.Animate.Disabled = true
                            plr.Character.Humanoid:Destroy()
                            Humanoid.Parent = plr.Character
                            Tool.Parent = plr.Character
                            firetouchinterest(Target.Character.HumanoidRootPart, Tool.Handle, 0);
                            wait(0.1)
                            cmdlp.Character:Remove()
                            game.Players.localPlayer.CharacterAdded:Wait()
                            wait(.2)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(_G.Bring)
                            end
                        end)

                        Voidkill.MouseButton1Click:Connect(function()
                            local Target = Player(Target.Text)
                            local cmdp = game:GetService("Players")
                            local cmdlp = cmdp.LocalPlayer

                            if not game.Players.LocalPlayer.Backpack:FindFirstChild("potion") then
                            game.StarterGui:SetCore("SendNotification", {
                            Title = "Notification";
                            Text = "No Potion Detected";
                            Duration = 1;
                            })
                            end
                    
                    _G.VoidKill = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                    if game.Players.LocalPlayer.Backpack:FindFirstChild("potion") then
                    
                        

                            cmdlp.Character:Remove()
                            game.Players.localPlayer.CharacterAdded:Wait()
                            wait(0.3)
                            
                            if game.Players.LocalPlayer.Backpack:FindFirstChild("potion") then
                                game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("potion"))
                            end
                            wait(0.5)
                            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Tool'):Activate()
                            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Tool'):Activate()
                            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Tool'):Activate()
                            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Tool'):Activate()
                            wait(2.2)

                            if game.Players.LocalPlayer.Character:FindFirstChild("potion") then
                                game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("potion"))
                            end
                            wait()
                            game.Players.LocalPlayer.Backpack:FindFirstChild("Push"):Destroy()
                                cmdlp.Character.Humanoid.Name = 1
                                local l = cmdlp.Character["1"]:Clone()
                                l.Parent = cmdlp.Character
                                l.Name = "Humanoid"
                                wait(.2)
                                cmdlp.Character["1"]:Destroy()
                                game.Workspace.CurrentCamera.CameraSubject = plr.Character.Humanoid
                                cmdlp.Character.Humanoid.DisplayDistanceType = "None"
                                cmdlp.Character.Humanoid:UnequipTools()
                                local v = cmdlp.Backpack:FindFirstChildOfClass("Tool")
                                if not v then
                                    game.StarterGui:SetCore("SendNotification", {
                                        Title = "Notification";
                                        Text = "No Tools Has Been Detected";
                                        Duration = 5;
                                    })
                                    return
                                end
                                v.Parent = cmdlp.Character
                                v.Parent = cmdlp.Character.HumanoidRootPart
                                firetouchinterest(Target.Character.HumanoidRootPart, v.Handle, 0)
                                firetouchinterest(Target.Character.HumanoidRootPart, v.Handle, 1)
                                pcall(function() cmdlp.Character.HumanoidRootPart.CFrame = CFrame.new(0, workspace.FallenPartsDestroyHeight + 5, 0) end)
                                wait(.2)
                                cmdlp.Character:Remove()
                                game.Players.localPlayer.CharacterAdded:Wait()
                                wait(.2)
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(_G.VoidKill)
                            end
                        end)


                        Give.MouseButton1Click:Connect(function()

                            local some = Player(Target.Text)
                        
