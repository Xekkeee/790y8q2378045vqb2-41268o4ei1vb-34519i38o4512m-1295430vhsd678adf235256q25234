if game.Players.LocalPlayer.Backpack:FindFirstChild("[Double-Barrel SG]") then
    game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("[Double-Barrel SG]"))
