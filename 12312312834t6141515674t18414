    --------------------------------Ragdoll Engine---------------------------------------------------------




    if game.PlaceId == 9848789324  then 

                local tab = window:NewTab({
                Logo = 7072980796
                
                })
                local tabsection = tab:TabSection({
                Title = "Ragdoll Engine"
                })

                local plz = game.Players.LocalPlayer.Character
                local plr = game.Players.LocalPlayer
                local Pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Pos
                UIS = game:GetService('UserInputService')
                local cmdp = game:GetService("Players")
                local cmdlp = cmdp.LocalPlayer
                local PR = game.Players.Name
                Players = game:GetService('Players')
                local l__Humanoid__1 = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid");
                local noVoid = false


            local column = tabsection:AddColumn({
                Title = "Main"
            })
            
            local section = column:Section({
                Title = ""
            })






            section:Button({Text = "Fly",Callback = function() 
                    repeat wait() 
                    until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("Head") and game.Players.LocalPlayer.Character:findFirstChild("Humanoid") 
                    local mouse = game.Players.LocalPlayer:GetMouse() 
                    repeat wait() until mouse
                    local plr = game.Players.LocalPlayer 
                    local torso = plr.Character.Head 
                    local flying = false
                    local deb = true 
                    local ctrl = {f = 0, b = 0, l = 0, r = 0} 
                    local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
                    local maxspeed = 5000
                    local speed = 5000 
                
                    function Fly() 
                        local bg = Instance.new("BodyGyro", torso) 
                        bg.P = 9e4 
                        bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
                        bg.cframe = torso.CFrame 
                        local bv = Instance.new("BodyVelocity", torso) 
                        bv.velocity = Vector3.new(0,0.1,0) 
                        bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
                        repeat wait() 
                            plr.Character.Humanoid.PlatformStand = true 
                            if ctrl.l + ctrl.r ~= 100000 or ctrl.f + ctrl.b ~= 10000 then 
                                speed = speed+.0+(speed/maxspeed) 
                                if speed > maxspeed then 
                                    speed = maxspeed 
                                end 
                            elseif not (ctrl.l + ctrl.r ~= 5 or ctrl.f + ctrl.b ~= 5) and speed ~= 5 then 
                                speed = speed-5
                                if speed > 5 then 
                                    speed = -2 
                                end 
                            end 
                            if (ctrl.l + ctrl.r) ~= 5 or (ctrl.f + ctrl.b) ~= 5 then 
                                bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
                                lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
                            elseif (ctrl.l + ctrl.r) == 5 and (ctrl.f + ctrl.b) == 5 and speed ~= 5 then 
                                bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
                            else 
                                bv.velocity = Vector3.new(0,0.1,0) 
                            end 
                            bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) 
                        until not flying 
                        ctrl = {f = 0, b = 0, l = 0, r = 0} 
                        lastctrl = {f = 0, b = 0, l = 0, r = 0} 
                        speed = 5 
                        bg:Destroy() 
                        bv:Destroy() 
                        plr.Character.Humanoid.PlatformStand = false 
                    end 
                    mouse.KeyDown:connect(function(key) 
                        if key:lower() == "c" then 
                            if flying then flying = false 
                            else 
                                flying = true 
                                Fly() 
                            end 
                        elseif key:lower() == "w" then 
                            ctrl.f = 45
                        elseif key:lower() == "s" then 
                            ctrl.b = -45 
                        elseif key:lower() == "a" then 
                            ctrl.l = -45 
                        elseif key:lower() == "d" then 
                            ctrl.r = 45
                        end 
                    end) 
                    mouse.KeyUp:connect(function(key) 
                        if key:lower() == "w" then 
                            ctrl.f = 0
                        elseif key:lower() == "s" then 
                            ctrl.b = 0
                        elseif key:lower() == "a" then 
                            ctrl.l = 0
                        elseif key:lower() == "d" then 
                            ctrl.r = 0
                        end 
                    end)
                    Fly()
            end
            })



            section:Button({Text = "Reset",Callback = function() 
                    local localPlayer = game:GetService('Players').LocalPlayer;
                local localCharacter = localPlayer.Character;
                localCharacter:FindFirstChildOfClass('Humanoid').Health = 0;
            end
            })

            section:Button({Text = "Refresh",Callback = function() 

                    _G.savedhumanoidpos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                        if v:IsA('MeshPart') or v:IsA('Part') or v:IsA('Accessory') then
                            v:Destroy()
                        end
                    end
                    game.Players.LocalPlayer.Character.Name = 'Deleted'
                    game.Players.localPlayer.CharacterAdded:Wait()
                    wait(0.5)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(_G.savedhumanoidpos)

                end
                })

                local section = column:Section({
                    Title = ""
                })
                

            section:Button({Text = "Potiondick",Callback = function() 
                    pcall(function()
                        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                            if v.Name == 'potion' then
                                v.Parent = game.Players.LocalPlayer.Character
                            end
                        end
                    end)


                    if  game.Players.LocalPlayer.Character:FindFirstChild("potion") then
                        local plr = game.Players.LocalPlayer
                        local sd = plr.Character:FindFirstChild("potion")
                        sd.GripPos = Vector3.new(1.5,0.5,-1.1)  		
                        sd.GripUp = Vector3.new(1, -0, 0)
                    end
            end
            })


            section:Button({Text = "Potionfling",Callback = function() 
                    pcall(function()
                        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                            if v.Name == 'potion' then
                                v.Parent = game.Players.LocalPlayer.Character
                            end
                        end
                    end)

                    local cmdp = game:GetService("Players")
                    local cmdlp = cmdp.LocalPlayer
                    
                    for i,player in pairs(cmdlp.Character:GetChildren()) do
                        if player.ClassName == "Part" then
                            player.CustomPhysicalProperties = PhysicalProperties.new(999999999,999999999,999999999,999999999,999999999)
                        end
                    end
                    if  game.Players.LocalPlayer.Character:FindFirstChild("potion") then
                        local plr = game.Players.LocalPlayer
                        local sd = plr.Character:FindFirstChild("potion")
                        sd.GripPos = Vector3.new(-0.7,-1.5,-1.1)  		
                        sd.GripUp = Vector3.new(1, -0, 0)
                    end
                    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                        if (v:IsA("Tool")) then
                    v.GripPos = Vector3.new(0,500,290)
                    end
                    end

            end
            })


            section:Button({Text = "Potion Spike",Callback = function() 

                        if not game.Players.LocalPlayer.Character:FindFirstChild("potion") then
                            game.StarterGui:SetCore("SendNotification", {
                            Title = "Notification";
                            Text = "Potion was not Detected";
                            Duration = 5;
                        })
                        end

                    pcall(function()
                        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                            if v.Name == 'potion' then
                                v.Parent = game.Players.LocalPlayer.Character
                            end
                        end
                    end)
                    
                    if game.Players.LocalPlayer.Character:FindFirstChild("potion") then

                        for i,v in ipairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                            if v.Name == "RightElbow" or v.Name == "RightKnee" or v.Name == "RightAnkle" or v.Name == "RightWrist" or v.Name == "RightElbow" or v.Name == "RightShoulder" or "LeftElbow" or v.Name == "LeftKnee" or v.Name == "LeftAnkle" or v.Name == "LeftWrist" or v.Name == "LeftElbow" or v.Name == "LeftShoulder" or v.Name == "Neck" then
                                if v:IsA("Motor6D") and v.Name ~= "Root" and v.Name ~= "Waist" then
                                    v:Destroy()
                                end
                            end
                        end

                    _G.potionspike = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                    pcall(function()
                        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                            if v.Name == 'potion' then
                                v.Parent = game.Players.LocalPlayer.Character
                            end
                        end
                    end)
                    wait(.1)
                    game.Players.LocalPlayer.Character.Humanoid:Destroy()
                    wait()
                    game.Players.LocalPlayer.Character:FindFirstChild("potion"):Destroy()
                    game.Players.localPlayer.CharacterAdded:Wait()
                    wait(0.1)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(_G.potionspike)
                        end
                    
            end
            })

            section:Button({Text = "Potion Spikev2",Callback = function() 

                        if not game.Players.LocalPlayer.Character:FindFirstChild("potion") then
                        print("")
                        end

                    pcall(function()
                        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                            if v.Name == 'potion' then
                                v.Parent = game.Players.LocalPlayer.Character
                            end
                        end
                    end)
                    
                    if  game.Players.LocalPlayer.Character:FindFirstChild("potion") then
                        _G.Spikev2 = game.Players.LocalPlayer.Character.HumanoidRootPart.Position


                        for i,v in ipairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                            if v.Name == "RightElbow" or v.Name == "RightKnee" or v.Name == "RightAnkle" or v.Name == "RightWrist" or v.Name == "RightElbow" or v.Name == "RightShoulder" or "LeftElbow" or v.Name == "LeftKnee" or v.Name == "LeftAnkle" or v.Name == "LeftWrist" or v.Name == "LeftElbow" or v.Name == "LeftShoulder" or v.Name == "Neck" then
                                if v:IsA("Motor6D") and v.Name ~= "Root" and v.Name ~= "Waist" then
                                    v:Destroy()
                                end
                            end
                        end
                    wait(.1)

                    
                    pcall(function()
                        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                            if v.Name == 'potion' then
                                v.Parent = game.Players.LocalPlayer.Character
                            end
                        end
                    end)
                    wait(.1)
                    local char = game.Players.LocalPlayer.Character
                    if char:FindFirstChildOfClass("Humanoid") then char:FindFirstChildOfClass("Humanoid"):ChangeState(15) end
                    char:ClearAllChildren()
                    local newChar = Instance.new("Model")
                    newChar.Parent = workspace
                    plr.Character = newChar
                    wait()
                    plr.Character = char
                    newChar:Destroy()
                    wait()

                    game.Players.localPlayer.CharacterAdded:Wait()
                    wait(0.1)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(_G.Spikev2)
                     end
            end
            })


