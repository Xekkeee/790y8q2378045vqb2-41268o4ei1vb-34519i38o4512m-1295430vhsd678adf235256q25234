                        wait(1)
                        game:GetService('RunService'):BindToRenderStep("Killrake", 0 , function(v)  
                            
                            for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                                if v:IsA("Tool") then
                                v.Parent = game.Players.LocalPlayer.Character
                                end
                            end
                            wait(.1)
                                if game.Players.LocalPlayer.Character:FindFirstChild("MegaFish") then
                                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("MegaFish"))
                                end
                
                
                        if game.Players.LocalPlayer.Backpack:FindFirstChild("MegaFish") then
                                game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("MegaFish"))
                            end
                
                
                        if game.Players.LocalPlayer.Character:FindFirstChild("MegaFish") then
                                game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("MegaFish"))
                            end
                
                            if game.Players.LocalPlayer.Character:FindFirstChild("MegaFish") then
                                game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("MegaFish"))
                            end
                        
                                    if game.Players.LocalPlayer.Backpack:FindFirstChild("Pan") then
                                game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("MegaFish"))
                                    end
                        wait()
                        if game.Players.LocalPlayer.Character:FindFirstChild("Pan") then
                            game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("Pan"))
                        end
                                end)
                    elseif v == false then
                        game:GetService('RunService'):UnbindFromRenderStep("Killrake")
                    end
                end
                })









