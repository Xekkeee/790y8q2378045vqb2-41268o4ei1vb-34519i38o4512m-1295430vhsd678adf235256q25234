                        game:GetService('RunService'):BindToRenderStep("Anti-Taser", 0 , function()
                            local v = game.Players.LocalPlayer.Character.UpperTorso:FindFirstChild("ElectricuteParticle") 
                                    if v then v:Destroy()
                                        end
                                                        for _, v in next, game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):GetPlayingAnimationTracks() do
                                                            if (v.Animation.AnimationId:match("rbxassetid://5641749824")) then
                                                                v:Stop();
                                                            end;
                                                        end;	           
                                end)
                                elseif v == false then
                                    game:GetService('RunService'):UnbindFromRenderStep("Anti-Taser")
