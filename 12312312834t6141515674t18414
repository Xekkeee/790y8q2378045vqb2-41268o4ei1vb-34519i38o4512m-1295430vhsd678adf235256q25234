                    section:Button({Text = "SuperHuman (Z)",Callback = function() 
                            game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
                                if key == "z" then
                                    local A_1 = game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("SuperSpeed")
                                    local A_2 = game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("HulkJump")
                                    if A_1 then
                                        game:GetService"Players".LocalPlayer.Character.BodyEffects.Movement.SuperSpeed:Destroy()
                                    else
                                        local OD2 = Instance.new("IntValue",game:GetService"Players".LocalPlayer.Character.BodyEffects.Movement)
                                        OD2.Name = "SuperSpeed"
                                    end
                                    if A_2 then
                                        game:GetService"Players".LocalPlayer.Character.BodyEffects.Movement.HulkJump:Destroy()
                                    else
                                        local OD1 = Instance.new("IntValue",game:GetService"Players".LocalPlayer.Character.BodyEffects.Movement)
                                        OD1.Name = "HulkJump"
                                    end
                                end
                            end)
                    end
                    })

                    section:Button({Text = "SpeedGlitch (C)",Callback = function() 
                        plr = game:GetService('Players').LocalPlayer
                        down = true
                        function onButton1Down(mouse)
                            down = true
                            while down do
                                if not down then break end
                                local char = plr.Character
                                char.HumanoidRootPart.Velocity = char.HumanoidRootPart.CFrame.lookVector * 230
                                wait()
                            end
                        end
                        function onButton1Up(mouse)
                            down = false
                        end
                        function onSelected(mouse)
                            mouse.KeyDown:connect(function(z) if z:lower()=="c"then onButton1Down(mouse)end end)
                            mouse.KeyUp:connect(function(z) if z:lower()=="c"then onButton1Up(mouse)end end)
                        end
                        onSelected(game.Players.LocalPlayer:GetMouse())
                    end
                    })

