                        local function XJXFBO_fake_script() -- Counter.LocalScript 
                            local script = Instance.new('LocalScript', Counter)
                            game.RunService.Stepped:Connect(function()
                                Counter.Text = XD.."/1250"
                            end)
                        end
                        coroutine.wrap(XJXFBO_fake_script)()

                        local function LGCZY_fake_script() -- FPS.FPSHandler 
                            local script = Instance.new('LocalScript', FPS)

                            local textLabel = script.Parent
                            local Delay = false
                            local Last = tick()
                            
                            game:GetService("RunService").RenderStepped:Connect(function()
                                local FramePerSec = math.floor(.5+(1/(tick() - Last )))
                                if Delay == false then
                                    Delay = true
                                    textLabel.Text = FramePerSec.." FPS"
                                    wait(1)
                                    Delay = false
                                end
                                Last = tick()
                            end)
                        end
                        coroutine.wrap(LGCZY_fake_script)()
                        local function PPYVV_fake_script() -- PING.Script 
                            local script = Instance.new('Script', PING)


                            function GetPing()
                                local Ping = 300-((1/wait())*10)
                                if Ping < 1 then
                                    Ping = 1
                                end
                                return math.floor(Ping)
                            end
                            
                            while wait(1) do
                                script.Parent.Text = GetPing().." ms"
                            end
                        end

                        coroutine.wrap(PPYVV_fake_script)()
                        local function CCXSRZ_fake_script() -- Timer.TimerScript 
                            local script = Instance.new('LocalScript', Timer)

                            local timer = script.Parent
                            local minutes = 5
                            local seconds = 0
                            
                            repeat
                                if seconds <= 0 then
                                    minutes = minutes - 1
                                    seconds = 59
                                else
                                    seconds = seconds - 1
                                end
                                if seconds < 10 then
                                    timer.Text = tostring(minutes)..":0"..tostring(seconds)
                                else 
                                    timer.Text = tostring(minutes)..":"..tostring(seconds)
                                end
                                wait(1)
                            until minutes <= 0 and seconds <= 0
                        end
                        coroutine.wrap(CCXSRZ_fake_script)()
                end
                })



