    section:Button({Text = "Anti-AFK",Callback = function() 
        Notification:Notify(
        { Title = "Notification", Description = "Your going to AFK Mode,                   you will only get Interupted if you get Disconnected out from the Game"},
        {OutlineColor = Color3.fromRGB(0,0,188),Time = 10, Type = "image"},
        {Image = "http://www.roblox.com/asset/?id=7072706318", ImageColor = Color3.fromRGB(0,0,188), Callback = function(State) print(tostring(State)) end}
        ) 
                while not game:IsLoaded() do wait() end
            repeat wait() until game.Players.LocalPlayer.Character
            Players = game:GetService("Players")
            local GC = getconnections or get_signal_cons
            if GC then
                for i,v in pairs(GC(Players.LocalPlayer.Idled)) do
                    if v["Disable"] then v["Disable"](v)
                    elseif v["Disconnect"] then v["Disconnect"](v)
                    end
                end
            else
            Players.LocalPlayer.Idled:Connect(function()
                VirtualUser:CaptureController()
                VirtualUser:ClickButton2(Vector2.new())
            end)
            end
    end
    })

    section:Button({Text = "Anti-Kick",Callback = function() 

        Notification:Notify(
            { Title = "Notification", Description = "Warning! this only Effects LocalPlayer Client/Script Kick."},
            {OutlineColor = Color3.fromRGB(0,0,188),Time = 10, Type = "image"},
            {Image = "http://www.roblox.com/asset/?id=7072706318", ImageColor = Color3.fromRGB(0,0,188), Callback = function(State) print(tostring(State)) end}
        ) 

        local mt = getrawmetatable(game)
        local old = mt.__namecall
        local protect = newcclosure or protect_function
    
        if not protect then
            notify("Incompatible Exploit Warning", "Your exploit does not support protection against stack trace errors, resulting to fallback function")
            protect = function(f) return f end
        end
    
        setreadonly(mt, false)
        mt.__namecall = protect(function(self, ...)
            local method = getnamecallmethod()
            if method == "Kick" then
                wait(9e9)
                return
            end
            return old(self, ...)
        end)
    end
    })


