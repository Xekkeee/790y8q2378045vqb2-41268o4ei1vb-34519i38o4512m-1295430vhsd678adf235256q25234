local LocalPlayer = game.Players.LocalPlayer

LocalPlayer.CharacterAdded:Connect(function()
    if game.Players.LocalPlayer then
        game.Players.LocalPlayer.Character:WaitForChild("BodyEffects")
    local success, err = pcall(function()

plr.Character.Animate.idle.Animation1.AnimationId = AnimationModule[AnimationState.Idle][1]
plr.Character.Animate.idle.Animation2.AnimationId = AnimationModule[AnimationState.Idle][2]
plr.Character.Animate.walk.WalkAnim.AnimationId = AnimationModule[AnimationState.Walk][3]
plr.Character.Animate.run.RunAnim.AnimationId = AnimationModule[AnimationState.Run][4]
plr.Character.Animate.jump.JumpAnim.AnimationId = AnimationModule[AnimationState.Jump][5]
plr.Character.Animate.climb.ClimbAnim.AnimationId = AnimationModule[AnimationState.Climb][6]
plr.Character.Animate.fall.FallAnim.AnimationId = AnimationModule[AnimationState.Fall][7]
for i, v in pairs(AnimationState) do
    print(i, v)
end

end)
end
end)



