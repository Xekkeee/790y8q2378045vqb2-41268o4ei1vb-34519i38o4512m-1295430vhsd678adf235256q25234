plr.Character.HumanoidRootPart.CFrame = CFrame.new(savedarmourpos)
function bypass()
    local oh1 = CFrame.new(681.7394409179688, 194.24996948242188, -405.8536071777344)
    local oh2 = game:GetService("Players")
    local oh3 = oh2.LocalPlayer.Character.HumanoidRootPart

    oh3.CFrame = oh1
    wait(1.5)
end
repeat
    wait()
until game:IsLoaded()

if workspace.Players[game.Players.LocalPlayer.Name]:FindFirstChild("[Double-Barrel SG]") then
    workspace.Players[game.Players.LocalPlayer.Name]:FindFirstChild("[Double-Barrel SG]").Name = "[X]"

    function Buy()
        fireclickdetector(workspace.Ignored.Shop["18 [Double-Barrel SG Ammo] - $60"].ClickDetector)
        wait(.3)
        fireclickdetector(workspace.Ignored.Shop["18 [Double-Barrel SG Ammo] - $60"].ClickDetector)
        wait(.3)
        fireclickdetector(workspace.Ignored.Shop["18 [Double-Barrel SG Ammo] - $60"].ClickDetector)
        wait(.3)
