Tab:AddButton({Name = "Bag",Callback = function()
    if Select then
    function getRoot(char)
        local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
        return rootPart
    end
    if Select and game.Players[Select].Character.BodyEffects['K.O'].Value == false then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Ignored.Shop['[BrownBag] - $25'].Head.CFrame
        wait(.30)
        fireclickdetector(game.Workspace.Ignored.Shop['[BrownBag] - $25'].ClickDetector)
        game.Players.LocalPlayer.Backpack:WaitForChild("[BrownBag]").Parent = game.Players.LocalPlayer.Character
        local c = "Attempting to Bag  " .. Select .. "." 
        OrionLib:MakeNotification({
            Name = "Notification",
            Content = c,
            Time = 5
        }) 
        wait(.5)
        repeat
            pcall(function()
                wait()
                getRoot(game.Players[Select].Character).CFrame = getRoot(game.Players.LocalPlayer.Character).CFrame + Vector3.new(1,3,0)
                game.Players.LocalPlayer.Character["[BrownBag]"]:Activate()
            end)
        until game.Players[Select].Character:FindFirstChild("Christmas_Sock")
        local c = "Successfully Bagged  " .. Select .. "."
        OrionLib:MakeNotification({
            Name = "Notification",
            Content = c,
            Time = 5
        }) 
    elseif game.Players[Select].Character.BodyEffects['K.O'].Value == false then
        local c = "" .. Select .. " Is Already Bagged." 
        OrionLib:MakeNotification({
            Name = "Notification",
            Content = c,
            Time = 5
        }) 
    end
end
end    
})

Tab:AddButton({Name = "Knock",Callback = function()
    if Select then
    local localPlayer     = game:GetService("Players").LocalPlayer;
    local localCharacter  = localPlayer.Character;
    
    if Select and game.Players[Select].Character.BodyEffects['K.O'].Value == false then
        pcall(function()
            localCharacter.LeftHand.LeftWrist:Destroy();
            localCharacter.RightHand.RightWrist:Destroy();
        end);
        repeat
            wait();
            if game.Players.LocalPlayer.Backpack:FindFirstChild("Combat") then
                game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("Combat"))
            end
            
            localCharacter.LeftHand.CFrame = game.Players[Select].Character.LowerTorso.CFrame;
            localCharacter.RightHand.CFrame = game.Players[Select].Character.LowerTorso.CFrame;
            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Tool'):Activate()
            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').JumpPower = 0
            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').HipHeight = 6
        until game.Players[Select].Character.BodyEffects['K.O'].Value == true
        game:GetService("Players").LocalPlayer.Character.LeftHand.CFrame  = game:GetService("Players").LocalPlayer.Character.LeftLowerArm.CFrame
        game:GetService("Players").LocalPlayer.Character.RightHand.CFrame = game:GetService("Players").LocalPlayer.Character.RightLowerArm.CFrame
        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').HipHeight = 2
        local c = Select .. " got Knocked." 
        OrionLib:MakeNotification({
            Name = "Notification",
            Content = c,
            Time = 5
        }) 
    elseif game.Players[Select].Character.BodyEffects['K.O'].Value == true then
        game:GetService("Players").LocalPlayer.Character.LeftHand.CFrame  = game:GetService("Players").LocalPlayer.Character.LeftLowerArm.CFrame
        game:GetService("Players").LocalPlayer.Character.RightHand.CFrame = game:GetService("Players").LocalPlayer.Character.RightLowerArm.CFrame
        local c = "" .. Select .. " is Already Knocked." 
