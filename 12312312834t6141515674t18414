Tab:AddToggle({Name = "Legit AntiLock",Default = false,Callback = function(v)
    Duration = 2;
    AntiLock = true
    local hip = 2.6
    local val = -40
    local cmdp = game:GetService("Players")
    local cmdlp = cmdp.LocalPlayer

    function AA()
        local oldVelocity = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(oldVelocity.X, val, oldVelocity.Z)
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(oldVelocity.X, oldVelocity.Y, oldVelocity.Z)
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(oldVelocity.X, val, oldVelocity.Z)
        game.Players.LocalPlayer.Character.Humanoid.HipHeight = hip
    end
    
    if v == true then
        getgenv().LegtiAA = v
        for i,player in pairs(cmdlp.Character:GetChildren()) do
            if player.ClassName == "Part" then
                player.CustomPhysicalProperties = PhysicalProperties.new(math.huge,math.huge,math.huge,math.huge,math.huge)
            end
        end
        AntiAIM()
        game.Players.LocalPlayer.Character.Humanoid.HipHeight = hip
        local Loop
        local loopFunction = function()
            if game.Players.LocalPlayer then
                game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
                game.Players.LocalPlayer.Character:WaitForChild("BodyEffects")
                local success, err = pcall(function()
            AA()
            if game.Players.LocalPlayer.Character.Humanoid.FloorMaterial == Enum.Material.Slate then
                --game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            end
            if game.Players.LocalPlayer.Character.Humanoid.FloorMaterial == Enum.Material.Air then
            end
            wait()
        end)
    end
        end;
        local Start = function()
            Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
        end;
        local Pause = function()
            Loop:Disconnect()
        end;
            Start()	
            repeat wait() until AntiLock == false
            Pause()
    elseif v == false then
        AntiLock = false
    end
end
})


getgenv().LegtiAA = false;
function AntiAIM()
    spawn(function()
        while wait() do
            if not getgenv().LegtiAA then break end;
            if game.Players.LocalPlayer then
                    game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
                    game.Players.LocalPlayer.Character:WaitForChild("BodyEffects")
                    local success, err = pcall(function()


                for i, v in next, Players:GetPlayers() do
                    if v ~= LocalPlayer and Alive(v) and Alive(LocalPlayer) then
                        local BodyEffects = v.Character:FindFirstChild("BodyEffects")
                        local MousePos = BodyEffects:FindFirstChild("MousePos")
                        if BodyEffects ~= nil and MousePos ~= nil then
                            local EnemyMouseMagnitude = (LocalPlayer.Character.HumanoidRootPart.Position - MousePos.Value).Magnitude
                            if PuppywareSettings.Blatant.LegitAA.AntiAimAtDistance > EnemyMouseMagnitude then
                                Root.CFrame = Root.CFrame * CFrame.new(math.random(4, 6) == 2 and 4 or -2, 0, 0)
                            end
                        end
                    end
                end


            end)

        end
        end
    end)
end

Tab:AddToggle({Name = "AntiLock v2",Default = false,Callback = function(v)

    PastedSources = false
    if v == true then

        checkcaller = checkcaller
        newcclosure = newcclosure
        hookmetamethod = hookmetamethod
        PastedSources = true
        local BruhXD = game:GetService("RunService")
        local LocalPlayer = game:GetService("Players").LocalPlayer
        local Bullshit = LocalPlayer:GetMouse()
    
        Bullshit.KeyDown:Connect(function(SayNoToOblivity)
            if SayNoToOblivity == ("=") then
                game:GetService("TeleportService"):Teleport(game.PlaceId, LocalPlayer) 
            end
        end)

        function RandomNumberRange(a)
            return math.random(-a * 100, a * 100) / 100
        end
         
        function RandomVectorRange(a, b, c)
            return Vector3.new(RandomNumberRange(a), RandomNumberRange(b), RandomNumberRange(c))
        end
        
        local DesyncTypes = {}
        BruhXD.Heartbeat:Connect(function()
            if PastedSources == true then
                local success, err = pcall(function()
                DesyncTypes[1] = LocalPlayer.Character.HumanoidRootPart.CFrame
                DesyncTypes[2] = LocalPlayer.Character.HumanoidRootPart.AssemblyLinearVelocity
         
                local SpoofThis = LocalPlayer.Character.HumanoidRootPart.CFrame
         
                SpoofThis = SpoofThis * CFrame.new(Vector3.new(0, 0, 0))
                SpoofThis = SpoofThis * CFrame.Angles(math.rad(RandomNumberRange(0)), math.rad(RandomNumberRange(5)), math.rad(RandomNumberRange(0)))
         
                LocalPlayer.Character.HumanoidRootPart.CFrame = SpoofThis
         
                LocalPlayer.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(10, 10, 10) * 16384
         
                BruhXD.RenderStepped:Wait()
         
                LocalPlayer.Character.HumanoidRootPart.CFrame = DesyncTypes[1]
                LocalPlayer.Character.HumanoidRootPart.AssemblyLinearVelocity = DesyncTypes[2]
                end)
            end
        end)
         
         
        --// Hook_CFrame
        local XDDDDDD = nil
        XDDDDDD = hookmetamethod(game, "__index", newcclosure(function(self, key)
            if PastedSources == true then
                if not checkcaller() then
                    if key == "CFrame" and PastedSources == true and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and LocalPlayer.Character:FindFirstChild("Humanoid") and LocalPlayer.Character:FindFirstChild("Humanoid").Health > 0 then
                        if self == LocalPlayer.Character.HumanoidRootPart then
                            return DesyncTypes[1] or CFrame.new()
                        elseif self == LocalPlayer.Character.Head then
                            return DesyncTypes[1] and DesyncTypes[1] + Vector3.new(0, LocalPlayer.Character.HumanoidRootPart.Size / 2 + 0.5, 0) or CFrame.new()
                        end
                    end
                end
            end
            return XDDDDDD(self, key)
        end))
         

        elseif v == false then
        
        PastedSources = false
    end
end
})




