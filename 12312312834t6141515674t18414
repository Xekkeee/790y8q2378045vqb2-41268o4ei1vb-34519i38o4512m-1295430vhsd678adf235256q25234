                                end
                            LP = game:GetService("Players").LocalPlayer
                            
                            for i,v in pairs(LP.Character:GetDescendants()) do
                            if v:IsA("Tool") then
                            v.Parent = LP
                            end
                            end
                    end
                    })

                    section:Button({Text = "Destroy Bag",Callback = function() 
                        game.Players.LocalPlayer.Character.Christmas_Sock:FindFirstChild('Weld'):Destroy() 
                    end
                    })

                    section:Button({Text = "Force Drop",Callback = function() 
                                local d = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                            game.Players.LocalPlayer.Character:FindFirstChild("GRABBING_CONSTRAINT"):Destroy()
                            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').Sit = true
                        wait(1.1)
                            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').Jump = true
                            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').Jump = true
                            game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(71.565, 142.926, -690.33)
                            game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(71.565, 142.926, -690.33)
                            game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(71.565, 142.926, -690.33)
                            game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(71.565, 142.926, -690.33)
                            game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(71.565, 142.926, -690.33)
                        game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(71.565, 142.926, -690.33)
                        game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(71.565, 142.926, -690.33)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(d)
                    end
                    })

                    section:Button({Text = "Mute Boombox",Callback = function() 
                            for i,v in pairs(game.Players:GetPlayers()) do
                                pcall(function()
                                    if v.Name ~= game.Players.LocalPlayer.Name and v.Character.LowerTorso:FindFirstChild("BOOMBOXSOUND").Playing then
                                        v:Stop()
                                    end
                                end)
                            end
                    end
                    })

    
































































                    local lp = game:FindService("Players").LocalPlayer

                    local function gplr(String)
                        local Found = {}
                        local strl = String:lower()
                        if strl == "all" then
                            for i,v in pairs(game:FindService("Players"):GetPlayers()) do
                                table.insert(Found,v)
                            end
                        elseif strl == "others" then
                            for i,v in pairs(game:FindService("Players"):GetPlayers()) do
                                if v.Name ~= lp.Name then
                                    table.insert(Found,v)
                                end
                            end 
                        elseif strl == "me" then
                            for i,v in pairs(game:FindService("Players"):GetPlayers()) do
                                if v.Name == lp.Name then
                                    table.insert(Found,v)
                                end
                            end 
                        else
                            for i,v in pairs(game:FindService("Players"):GetPlayers()) do
                                if v.Name:lower():sub(1, #String) == String:lower() then
                                    table.insert(Found,v)
                                end
                            end 
                        end
                        return Found 
                    end

                    function findplayer(stringg)
                        for _, v in pairs(game.Players:GetPlayers()) do
                                if stringg:lower() == (v.Name:lower()):sub(1, #stringg) then
                                        return v
                                elseif stringg:lower() == (v.DisplayName:lower()):sub(1, #stringg) then
                                        return v
                                end
                        end
                    end
                    
                    function GetPlayer(String)
                        local plr
                        local strl = String:lower()
                            for i, v in pairs(game:GetService("Players"):GetPlayers()) do
                                if v.Name:lower():sub(1, #String) == String:lower() then
                                    plr = v
                                end 
                            end
                            if String == "me" then
                                    plr = game.Players.LocalPlayer
                                end
                            if String == "" or String == " " then
                            plr = nil
                            end
                        return plr
                    end
                    
                    local frame = script.Parent
                    local player = game.Players.LocalPlayer

                    local column = tabsection:AddColumn({
                        Title = "Player"
                    })
                    
                    local section = column:Section({
                        Title = "Teleport to a Person                            "
                    })



                section:Textbox({Text = "Tp",Callback = function(txt)
                        
                    local plr = findplayer(txt)
                    if plr then
                        local Pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = plr.Character:FindFirstChild("UpperTorso").CFrame

                    end
                        end
                    })


                section:Button({Text = "TeleportBack",Callback = function() 
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Pos
                    end
                    })


                    local section = column:Section({
                        Title = "View Person                                        "
                    })


                section:Textbox({Text = "View",Callback = function(txt)

                    local plr = findplayer(txt)
                    if plr then
                        game.Workspace.CurrentCamera.CameraSubject = plr.Character.Humanoid

                    end
                        end
                    })

                section:Button({Text = "StopView",Callback = function() 

                    game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
                    end
                    })
                

                    local section = column:Section({
                        Title = "FlingStuffs                                          "
                    })


                section:Textbox({Text = "Fling",Callback = function(txt)
                    local Target = gplr(txt)
                    if Target[1] then
                        Target = Target[1]

                        local Loop
                        local OldFlingPos = player.Character.HumanoidRootPart.Position
                        local loopFunction = function()

                            local success,err = pcall(function()
                                local FlingEnemy = Target.Character
                                if FlingEnemy and player.Character then
                                    FlingTorso = FlingEnemy.UpperTorso
                                    local dis = 3.85
                                    local increase = 6
                                    if FlingEnemy.Humanoid.MoveDirection.X < 0 then
                                        xchange = -increase
                                    elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
                                        xchange = increase
                                    else
                                        xchange = 0
                                    end
                                    if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                                        zchange = -increase
                                    elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                                        zchange = increase
                                    else
                                        zchange = 0
                                    end          
                                    if player.Character then
                                        player.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
                                        player.Character.HumanoidRootPart.CFrame = CFrame.new(FlingTorso.Position.X + math.random(-dis,dis) + xchange, FlingTorso.Position.Y, FlingTorso.Position.Z + math.random(-dis,dis) + zchange) * CFrame.Angles(math.rad(player.Character.HumanoidRootPart.Orientation.X + 350),math.rad(player.Character.HumanoidRootPart.Orientation.Y + 200),math.rad(player.Character.HumanoidRootPart.Orientation.Z + 240))
                                        player.Character.HumanoidRootPart.Velocity = Vector3.new(50000000,50000000,50000000)
                                    end
                                end
                            end)
                            if err then
                                print('fling error : ' .. err)
                            end
                        end;
                        local Start = function()    
                            OldFlingPos = player.Character.HumanoidRootPart.Position
                            Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
                        end;
                        local Pause = function()
                            Loop:Disconnect()
                            local vectorZero = Vector3.new(0, 0, 0)
                            player.Character.PrimaryPart.Velocity = vectorZero
                            player.Character.PrimaryPart.RotVelocity = vectorZero
                            player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
                        local vectorZero = Vector3.new(0, 0, 0)
                        player.Character.PrimaryPart.Velocity = vectorZero
                        player.Character.PrimaryPart.RotVelocity = vectorZero
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
                    end;

                        Start()
                        wait(10)
                        Pause()

                        



                        
                    end

                    end
                    })






















                section:Textbox({Text = "BikeFling",Callback = function(txt)
                        
                        local Target = gplr(txt)
                        if Target[1] then
                            Target = Target[1]
                            
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.8922119140625, 21.999820709228516, -546.8743286132812)
                        wait(0.5)
                        fireclickdetector(game:GetService("Workspace").Ignored.Shop["[DuoBike] - $75"].ClickDetector)
                        fireclickdetector(game:GetService("Workspace").Ignored.Shop["[DuoBike] - $75"].ClickDetector)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.6552124023438, 26.234392166137695, -553.2313842773438)
                        
