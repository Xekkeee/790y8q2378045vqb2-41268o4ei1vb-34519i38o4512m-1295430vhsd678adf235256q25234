            if game.Players.LocalPlayer then
                game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
                game.Players.LocalPlayer.Character:WaitForChild("BodyEffects")
                local success, err = pcall(function()
                if game.Players.LocalPlayer.Character:FindFirstChild("GRABBING_CONSTRAINT") then
                    game.Players.LocalPlayer.Character:FindFirstChild("GRABBING_CONSTRAINT"):Destroy()
                    for _, v in next, game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):GetPlayingAnimationTracks() do
                        if (v.Animation.AnimationId:match("rbxassetid")) then
                            v:Stop()
                        end
                    end         
                game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').Sit = true     
                wait()
				game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').Sit = false
                game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').Jump = true
                game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').Jump = true
                end
            end)
    end
    end)
    elseif v == false then
        game:GetService('RunService'):UnbindFromRenderStep("Anti-Grab")
        end	
end    
})

